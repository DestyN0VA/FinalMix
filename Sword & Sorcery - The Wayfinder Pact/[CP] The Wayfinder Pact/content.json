{
  "Format": "2.7.2",
  
  "Changes": [
    {
      "LogName": "S&S WF: FileLoader",
      "Action": "Include",
      "FromFile": "Loaders/FileLoader.json"
    }
  ],

  "DynamicTokens": [
    //BASIC UTILITY TOKENS
    { "Name": "TodaysDate", "Value": "{{Season}} {{Day}}" },
    { "Name": "IsOddYear", "Value": "{{Query: {{Year}} % 2 = 1}}" },

    { "Name": "WeekDayList", "Value": "Sun, Mon, Tue, Wed, Thu, Fri, Sat" },
    { "Name": "WeekDay", "Value": "{{WeekDayList |valueAt={{Query: {{Day}}%7}}}}" },

    { "Name": "YearCheck", "Value": 2 },
    { "Name": "YearCheck", "Value": 1, "When": { "Year": 1 } },

    { "Name": "FestivalYearCheck", "Value": "" },
    { "Name": "FestivalYearCheck", "Value": "_y2",
      "When": { "Query: {{Year}} % 2 = 1": false } }
  ],
  "ConfigSchema": {}
}